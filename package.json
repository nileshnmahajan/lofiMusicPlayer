{
  "name": "tesApp",
  "version": "0.0.1",
  "private": true,
  "scripts": {
    "android": "react-native run-android",
    "ios": "react-native run-ios",
    "start": "react-native start",
    "test": "jest",
    "lint": "eslint .",
    "adbrev": "adb reverse tcp:8081 tcp:8081",
    "metro": "react-native start",
    "connect": "npm run adbrev && npm run start",
    "bundle": "react-native bundle --dev false --platform android --entry-file index.js --bundle-output ./android/app/src/main/assets/index.android.bundle",
    "instD": "cd android/app/build/outputs/apk/debug && adb install app-arm64-v8a-debug.apk",
    "instR": "cd android/app/release && adb install app-universal-release.apk"
  },
  "dependencies": {
    "@react-native-async-storage/async-storage": "^1.17.0",
    "@react-native-community/masked-view": "^0.1.11",
    "@react-native-community/netinfo": "^8.2.0",
    "@react-native-community/slider": "^4.2.2",
    "@react-navigation/drawer": "^6.3.2",
    "@react-navigation/material-bottom-tabs": "^6.1.1",
    "@react-navigation/native": "^6.0.8",
    "@react-navigation/stack": "^6.1.1",
    "axios": "^0.26.1",
    "expo-linear-gradient": "^11.0.3",
    "expo-sqlite-orm": "^1.5.6",
    "haversine-geolocation": "^1.6.0",
    "moment": "^2.29.3",
    "react": "17.0.2",
    "react-native": "0.68.2",
    "react-native-actions-sheet": "^0.6.1",
    "react-native-gesture-handler": "^1.10.3",
    "react-native-image-placeholder": "^1.0.14",
    "react-native-localization": "^2.3.1",
    "react-native-marquee": "^0.4.0",
    "react-native-music-control": "^1.4.1",
    "react-native-paper": "^4.11.2",
    "react-native-paper-dropdown": "^1.0.7",
    "react-native-reanimated": "^3.0.0-rc.0",
    "react-native-render-html": "^6.3.4",
    "react-native-safe-area-context": "^4.2.2",
    "react-native-scaling-drawer": "^1.0.1",
    "react-native-screens": "^3.13.1",
    "react-native-sound": "^0.11.2",
    "react-native-sqlite-2": "^3.5.2",
    "react-native-track-player": "^2.1.3",
    "react-redux": "^8.0.2",
    "redux": "^4.1.2",
    "redux-mock-store": "^1.5.4",
    "redux-persist": "^6.0.0"
  },
  "devDependencies": {
